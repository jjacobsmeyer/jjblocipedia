<h1>Wikis</h1>
<div class="row">
  <div class="col-md-8">
    <% @wikis.each do |wiki| %>
    <div class="media">
      <div class="media-body">
        <h4 class="media-heading">
          <%= "Private: " if wiki.private %>
          <%= link_to wiki.title, wiki %>
        </h4>
      </div>
    </div>
    <% end %>
    <%= will_paginate @wikis %>
  </div>
</div>
